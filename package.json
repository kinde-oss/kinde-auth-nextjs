{
  "name": "@kinde-oss/kinde-auth-nextjs",
  "version": "2.11.1-1",
  "description": "Kinde Auth SDK for NextJS",
  "main": "dist/cjs/index.js",
  "module": "dist/index.js",
  "typings": "dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/src/index.d.ts",
        "default": "./dist/index.es.js"
      },
      "require": {
        "types": "./dist/types/src/index.d.ts",
        "default": "./dist/index.cjs.js"
      }
    },
    "./server": {
      "import": {
        "types": "./dist/types/src/server/index.d.ts",
        "default": "./dist/server.es.js"
      },
      "require": {
        "types": "./dist/types/src/server/index.d.ts",
        "default": "./dist/server.cjs.js"
      }
    },
    "./components": {
      "import": {
        "types": "./dist/types/src/components/index.d.ts",
        "default": "./dist/components.es.js"
      },
      "require": {
        "types": "./dist/types/src/components/index.d.ts",
        "default": "./dist/components.cjs.js"
      }
    },
    "./middleware": {
      "import": {
        "types": "./dist/types/src/middleware/index.d.ts",
        "default": "./dist/middleware.es.js"
      },
      "require": {
        "types": "./dist/types/src/middleware/index.d.ts",
        "default": "./dist/middleware.cjs.js"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/types/src/types.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "vite build",
    "build:watch": "vite build --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "release": "release-it",
    "lint": "prettier --check .",
    "lint:fix": "prettier --write .",
    "dev": "cd playground && pnpm run dev",
    "dev:prepare": "rm -rf playground && git clone https://github.com/kinde-starter-kits/kinde-nextjs-app-router-starter-kit playground && npm link && cd playground && npm i && mv .env_sample .env && rm -rf .git && npm link @kinde-oss/kinde-auth-nextjs"
  },
  "author": {
    "name": "Kinde",
    "email": "engineering@kinde.com",
    "url": "https://kinde.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kinde-oss/kinde-auth-nextjs"
  },
  "bugs": "https://github.com/kinde-oss/kinde-auth-nextjs",
  "homepage": "https://kinde.com",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.22.0",
    "@testing-library/react": "^16.2.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "4.0.3",
    "ajv": "^8.17.1",
    "changelogen": "^0.6.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.0.1",
    "genversion": "^3.2.0",
    "globals": "^16.0.0",
    "happy-dom": "^18.0.0",
    "prettier": "^3.4.2",
    "release-it": "^19.0.0",
    "rollup-plugin-preserve-directives": "^0.4.0",
    "typescript": "^5.7.3",
    "vite": "^6.2.5",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^4.0.0"
  },
  "peerDependencies": {
    "next": "^12.3.5 || ^13.5.9 || ^14.2.25 || ^15.2.3 || ^16.0.0",
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "dependencies": {
    "@kinde-oss/kinde-auth-react": "5.10.1",
    "@kinde-oss/kinde-typescript-sdk": "2.13.1",
    "@kinde/jwt-decoder": "^0.2.0",
    "@kinde/jwt-validator": "^0.4.0",
    "cookie": "^1.0.2",
    "crypto-js": "^4.2.0",
    "destr": "^2.0.5",
    "uncrypto": "^0.1.3"
  },
  "files": [
    "LICENSE.md",
    "package.json",
    "package-lock.json",
    "readme.md",
    "dist"
  ],
  "packageManager": "pnpm@10.22.0"
}
